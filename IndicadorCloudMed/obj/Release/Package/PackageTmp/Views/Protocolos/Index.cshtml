@model IndicadorGedWeb.Models.Protocolos

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Content/odometer-theme-car.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/Protocolos.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/odometer.js")"> </script>
<header>
    <h2>IEPC</h2>
</header>
<div class="container" id="container">
    <div class="section group">
        <div class="col span_40">
            <input type="hidden" id="IdEmpresa" value="0" />
            <table>
                <tr>
                    <th>Empresa</th>
                </tr>
                <tr>
                    <td>@Html.RadioButton("Empresa", 0, true)</td>
                    <td>Todos</td>
                </tr>
                @foreach (var row in ViewBag.Empresas)
                {
                    bool sel = @row.Selected;
                    string id = @row.Value;
                    string val = @row.Text;
                    <tr>
                        <td>@Html.RadioButton("Empresa", id, sel)</td>
                        <td>@row.Text</td>
                    </tr>
                }
            </table>
        </div>
        <div class="col span_49">
            <center>
                <div id="odometer" class="odometer" style="font-size: 100px">
                </div>
            </center>
        </div>
    </div>
    <div class="col span_40">
    </div>
    <div class="col span_49">
        <center>
            <h1 style="font-family: 'Segoe UI', Frutiger, 'frutiger linotype', 'Dejavu Sans' , 'Helvetica Neue' , arial, sans-serif; font-weight: lighter; text-decoration: none; font-size: 25px;">Protocolos abertos</h1>
        </center>
    </div>
    <div class="col span_100">
    </div>
    <div class="col span_100">
    </div>
    <div class="col span_100">
    </div>
    <div class="col span_100">
    </div>
    <div class="col span_100">
    </div>
    <div class="col span_100">
    </div>
    <div class="col span_100">
    </div>
</div>
</div>